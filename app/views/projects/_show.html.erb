<div class='row featurette'>
	<div class='col-md-6'>
		<img class='featurette-image img-responsive' src='<%= @project.project_pic %>'>
	</div>
	<div class='col-md-6'>
		<h2 class='featurette-heading'><%= @project.project_name %></h2>
		<p><%= @project.project_location %></p>
		<p><%= @project.project_type %></p>
		<p>$<%= @project.project_amount %></p>

	</div>
</div>

<div class='row featurette'>
	<div class='col-md-12 project-description'>
		<p class='lead'><%= @project.project_description %></p>
	</div>
</div>

